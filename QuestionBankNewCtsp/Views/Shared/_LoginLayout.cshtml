<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>Login</title>

    <!-- Bootstrap -->
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/css/font-awesome.min.css" rel="stylesheet">
    @*<link href="~/Content/css/style.css" type="text/css" rel="stylesheet">*@

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

    <link href="~/Content/customcss/SS.css" rel="stylesheet" />
    <link href="~/Content/css/style.css" rel="stylesheet" />
</head>
<body>
    <div class="container" style="padding:0;">
        <div class="dashboard">
            <div class="row" style="margin:0;">          
                <div class="col-md-12">
                    <div class="right-col">
                        <div class="topbar">
                            <div class="left-content pull-left">
                                <div class="toggle pull-left">
                                    <a href="#"> <i class="fa fa-bars"></i></a>
                                </div>
                                <div class="heading pull-left">Question-Bank</div>
                                <div class="clearfix"></div>
                            </div>
                            <div class="right-content pull-right">
                               
                            </div>
                            <div class="clearfix"></div>
                        </div>

                        <div class="row grid-second">
                            <div class="col-md-12 col-sm-12 col-xs-12">
                                <div class="g_panel">                                
                                    <div>
                                        @RenderBody()
                                        @Scripts.Render("~/bundles/bootstrap")
                                        @Scripts.Render("~/bundles/ckeditor")
                                        @RenderSection("scripts", required: false)
                                    </div>

                                </div>
                            </div>

                        </div>
                    </div>

                </div>
            </div>
            <!-- /right-col -->

            <div class="clearfix"></div>
        </div>

        <div class="footer">
            <div class="copyright pull-left">
                Copyright Saaddev
            </div>
            <div class="terms pull-right">
                <a href="">Terms & Conditions</a>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>


    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="~/Content/js/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="~/Content/js/bootstrap.min.js"></script>

  
</body>
</html>
